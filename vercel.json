{
  "crons": [
    {
      "path": "/",
      "schedule": "0 0 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/github",
      "destination": "https://github.com/jmigac"
    },
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/jmigac/"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
